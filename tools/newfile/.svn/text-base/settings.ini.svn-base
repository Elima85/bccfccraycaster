; All files should be entered without leading ".." as the application tries to find the correct root by itself
;
; Path to the file containing the Voreen GPL header
VoreenHeaderSource = "header.txt"
;
; Path to NewFile Directory
NewFileDirectory = "tools/newfile"
;
; Path to the project file of the core elements
PathToCorePro = "src/core/voreenlib_core.pro"
;
; Path to the project file of the qt elements
PathToQtPro = "src/qt/voreenlib_qt.pro"
;
; Path to core processors directory
CoreProcessorDirectory = "core/processors/"
;
; Path to the module's root directory
ModulesDirectory = "modules/"
;
; Suffix of module's project files
ModulesProExtension = "_core.pri"
;
; Path to VoreenModule class
VoreenModulePath = "voreen/core/voreenmodule.h"
;
; Path to VoreenModule registration file
ModuleRegistrationFile = "modules/moduleregistration.h"
